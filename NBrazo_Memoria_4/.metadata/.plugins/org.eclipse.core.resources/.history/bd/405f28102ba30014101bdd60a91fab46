void GLCD_Bmp (unsigned char *bmp) {
  unsigned int   j;
  unsigned short *bitmap_ptr = (unsigned short *)bmp;
  j=0;
  strcpy(cadena,"");
//  for (j = 0; j < 2*LARGO; j++) {
  while((bitmap_ptr != NULL) && (j<2*LARGO)){
	  //	  demo_messg_blank[j]=*bitmap_ptr++;
	  cadena[j++]=*bitmap_ptr++;
  }
}

