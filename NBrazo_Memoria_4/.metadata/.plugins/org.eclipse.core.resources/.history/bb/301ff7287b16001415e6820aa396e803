void GLCD_Bmp (unsigned char *bmp) {
  unsigned int   j;
  unsigned short *bitmap_ptr = (unsigned short *)bmp;
  char msg[4096];
/*
#if (HORIZONTAL == 1)
  x = WIDTH-x-w;
  GLCD_SetWindow(y, x, h, w);
#else
  GLCD_SetWindow(x, y, w, h);
#endif
  wr_cmd(0x22);
  wr_dat_start();
#if (HORIZONTAL == 1)
  bitmap_ptr += (h*w)-1;
  for (j = 0; j < h; j++) {
    for (i = 0; i < w; i++) {
      wr_dat_only(*bitmap_ptr--);
    }
  }
#else*/
  LPC_UART1->IER = IER_THRE | IER_RLS;

  //bitmap_ptr += 18;

  for (j = 0; j < 100000; j++) {
    //for (i = 0; i < w; i++) {
	  *bitmap_ptr++;
      if((*bitmap_ptr=='G')&&(*bitmap_ptr+1=='r')&&(*bitmap_ptr+2=='e')&&(*bitmap_ptr+3=='e'))UARTSend(1, (uint8_t *)"esta", 4 );//msg[j]=*bitmap_ptr++;
    //}
    //bitmap_ptr -= 2*w;
  }

  UART1Count = 0;
  LPC_UART1->IER = IER_THRE | IER_RLS | IER_RBR;
  //#endif
//  wr_dat_stop();
}
